import math

def soma(a, b):
    return a + b

def subtracao(a, b):
    return a - b

def multiplicacao(a, b):
    return a * b

def divisao(a, b):
    if b != 0:
        return a / b
    else:
        return "Não é possível dividir por zero"

def potencia(a, b):
    return a ** b

def raiz(a):
    return math.sqrt(a)

def logaritmo(a):
    return math.log(a)

def seno(a):
    return math.sin(a)

def cosseno(a):
    return math.cos(a)

def tangente(a):
    return math.tan(a)
